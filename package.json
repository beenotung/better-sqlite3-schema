{
  "name": "better-sqlite3-schema",
  "version": "1.1.0",
  "description": "Migrate (nested and multi-dimensional) json data to/from sqlite database with better-sqlite3-helper",
  "keywords": [
    "helper",
    "schema",
    "migrate",
    "import",
    "export",
    "better-sqlite3-helper",
    "better-sqlite3",
    "sqlite3"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "run-s format build jest",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "run-s clean tsc mk-package",
    "tsc": "tsc -p tsconfig.build.json",
    "tsc:watch":"tsc --noEmit --watch",
    "mk-package": "mk-package",
    "jest": "jest",
    "prepublishOnly": "npm run rawtest && npm run build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "better-sqlite3": "^7.4.3",
    "better-sqlite3-helper": "^3.1.4",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@beenotung/tslib": "^17.6.1",
    "@types/better-sqlite3": "^7.4.0",
    "@types/integer": "^4.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "*",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "mk-package": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsc-macro": "^0.1.3",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/better-sqlite3-schema.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/better-sqlite3-schema/issues"
  },
  "homepage": "https://github.com/beenotung/better-sqlite3-schema#readme"
}
